<?xml version="1.0"?>
<!DOCTYPE ACTIONS SYSTEM "actions.dtd">
<ACTIONS>
        <ACTION NAME="completion-complete">
                <CODE>
                completion.CompletionActions.completeFromCommand(view);
		</CODE>
        </ACTION>
        <ACTION NAME="completion-popup-cursor-down">
                <CODE>
                completion.CompletionActions.movePopupCursorDown();
		</CODE>
        </ACTION>
        <ACTION NAME="completion-popup-cursor-up">
                <CODE>
                completion.CompletionActions.movePopupCursorUp();
		</CODE>
        </ACTION>
</ACTIONS>